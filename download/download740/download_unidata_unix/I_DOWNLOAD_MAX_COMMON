      EQU DLMAX.FILES TO 10
      EQU DLMAX.DICT.RECS TO 300
      EQU DLMAX.LITERAL.VALUES TO 300
      EQU DLMAX.EVAL.FORMULAE TO 100
      EQU DLMAX.SUBR.NAMES TO 200
      EQU DLMAX.SUBR.ARGS TO 10
      EQU DLMAX.OUT.RECS TO 10
      EQU DLMAX.BREAK.FIELDS TO 25
